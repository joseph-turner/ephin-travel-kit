{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "typegen": {
      "outputs": [
        "sanity.types.ts"
      ]
    },
    "build": {
      "dependsOn": [
        "^build",
        "typegen"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "dependsOn": [
        "typegen"
      ],
      "cache": false,
      "persistent": true
    }
  }
}
